<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Menu </title>
</head>
<body>
    <nav>
        <a href="/">Home</a>
        <a href="/menu">Full Menu</a>
      </nav>
      
  <h1>Menu: <%= category %></h1>
  <% if (menuItems.length > 0) {%>
    
  <ul>
    <% menuItems.forEach(item => {%>
        <li>
            <strong><%= item.name %></strong> - $<%= item.price.toFixed(2) %>
            ( <%= item.rating %>) <br>
            <p><%= item.details %></p>
        </li>
        <%}) %>
  </ul>
  <%} else {%>
    <p>Sorry no items found in this category.</p>
<%}%>
  
</body>
</html>